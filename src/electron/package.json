{
  "name": "electron",
  "version": "0.0.0",
  "type": "commonjs",
  "main": "main.ts",
  "scripts": {
    "start": "concurrently \"npm run electron\" \"npm run watch\"",
    "electron": "tsc --project ./tsconfig.json && electron ./dist/src/electron/app.js",
    "watch": "tsc --watch --project ./tsconfig.json ",
    "createTables": "ts-node ./utilityRunners/tableUtilRunner.ts",
    "scanFiles": "ts-node ./utilityRunners/scanFilesRunner.ts",
    "clearTables": "ts-node ./utilityRunners/clearTablesRunner.ts"
  },
  "directories": {
    "buildResources": "dist",
    "app": "dist"
  },
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "create-react-app": "^5.0.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rxjs": "~7.8.0",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "web-vitals": "^2.1.4",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@types/electron": "^1.6.10",
    "@types/node": "^22.5.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "electron": "^23.2.0",
    "electron-log": "^5.1.2",
    "electron-reloader": "^1.2.3",
    "gulp": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "html-webpack-plugin": "^5.6.0",
    "os-browserify": "^0.3.0",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "util": "^0.12.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
