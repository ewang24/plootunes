{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "esModuleInterop": true,
    "strict": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "../../core/**/*.ts"
  ],
  "exclude": ["./node_modules"]
}
