{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "node",
    "removeComments": false,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "paths": {
      "*": [
        "*",
        "src/*",
        "../../global/*"
      ]
    }
  },
  "include": [
    "./view/**/*.tsx",
    "./view/**/*.ts",
    "../core/**/*.ts",
    "../core/**/*.js"
, "../core/assets/icons.ts"  ]
}