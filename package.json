{
  "name": "physic-octo-rotary-phone",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "react": "cd ./src/react && webpack-dev-server",
    "electron": "tsc --watch --project ./src/electron/tsconfig.json && electron ./src/electron/dist/app.js",
    "install:all": "npm install && cd ./src/electron && npm install && cd ../react && npm install",
    "scanFiles": "ts-node ./src/electron/utilityRunners/scanFilesRunner.ts",
    "createTables": "ts-node ./src/core/utilityRunners/tableUtilRunner.ts",
    "clearTables": "ts-node ./src/core/utilityRunners/clearTablesRunner.ts",
    "genTestLibrary": "ts-node ./src/core/db/test/testAudioFileGenerator.ts P:/Music/music/rotation/"
  },
  "private": true,
  "dependencies": {
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^22.5.3",
    "css-loader": "^7.1.1",
    "music-metadata": "^7.14.0",
    "node-id3": "^0.2.6",
    "react": "^18.2.0",
    "sass": "^1.75.0",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
